# 85000000000000
def calculate_power(base, exponent):
    # Check if the exponent is positive.
    if exponent < 0:
        raise ValueError("Exponent must be a positive number")
    
    # Calculate the power
    result = base ** exponent
    return result

# Example usage
result = calculate_power(2, 3)  # Calculate 2 to the power of 3
print(result)  # Display the result
